<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/my_nav"
    app:startDestination="@id/startFragment">

    <fragment
        android:id="@+id/startFragment"
        android:name="com.example.eventsconferencespj.log.StartFragment"
        android:label="fragment_start"
        tools:layout="@layout/fragment_start">
        <action
            android:id="@+id/action_startFragment_to_logIn_Fragment"
            app:destination="@id/logIn_Fragment" />
    </fragment>
    <!-- Đăng nhập-->
    <fragment
        android:id="@+id/logIn_Fragment"
        android:name="com.example.eventsconferencespj.log.LogIn_Fragment"
        android:label="fragment_register"
        tools:layout="@layout/fragment_register">
        <action
            android:id="@+id/action_logIn_Fragment_to_home2"
            app:destination="@id/home2" />
        <action
            android:id="@+id/action_logIn_Fragment_to_register_Fragment"
            app:destination="@id/register_Fragment" />
    </fragment>
    <!-- Đăng ký-->
    <fragment
        android:id="@+id/register_Fragment"
        android:name="com.example.eventsconferencespj.log.Register_Fragment"
        android:label="Register_Fragment">
        <action
            android:id="@+id/action_register_Fragment_to_logIn_Fragment"
            app:destination="@id/logIn_Fragment" />
    </fragment>


    <!-- Home-->
    <fragment
        android:id="@+id/home2"
        android:name="com.example.eventsconferencespj.Home"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" />

</navigation>